{
  "tags": {
    "endeavor-magazine-issue-item": {
      "template": "./item.marko",
      "@issue": "object",
      "@with-image": {
        "type": "boolean",
        "default-value": true
      },
      "@with-header": {
        "type": "boolean",
        "default-value": true
      },
      "@with-footer": {
        "type": "boolean",
        "default-value": true
      },
      "@with-title": {
        "type": "boolean",
        "default-value": true
      },
      "@with-description": {
        "type": "boolean",
        "default-value": false
