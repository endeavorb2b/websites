{
  "taglib-imports": [
    "./ads/marko.json",
    "./breadcrumbs/marko.json",
    "./content/marko.json",
    "./load-more/marko.json",
    "./magazine-publication/marko.json",
    "./magazine-issue/marko.json",
    "./magazine/marko.json",
    "./native-x/marko.json",
    "./navbars/marko.json",
    "./published-content/marko.json",
    "./search/marko.json",
    "./social/marko.json",
    "./subscribe/marko.json"
  ],
  "tags": {
    "endeavor-icon": {
      "template": "./icon"
    },
    "endeavor-load-icons": {
      "template": "./load-icons"
    },
    "endeavor-link": {
      "template": "./link"
    },
    "endeavor-item": {
      "template": "./item/item",
      "<image>": {
        "@alt": "string",
        "@display": {
          "type": "boolean",
          "default-value": true
        },
        "@image": "object",
        "@link-attrs": "object",
        "@link-to": "object",
        "@height": "number",
        "@lazyload": {
          "type": "boolean",
          "default-value": true
        },
        "@modifiers": "array",
        "@options": "object",
        "@position": {
          "type": "string",
          "default-value": "left"
        },
        "@width": "number",
        "@min-height": "number",
        "@min-width": "number",
        "@use-placeholder": {
          "type": "boolean",
          "default-value": true
        }
      },
      "<header-left>": {},
      "<header-right>": {},
      "<title>": {
        "@tag": {
          "type": "string",
          "default-value": "div"
        },
        "@modifiers": "array"
      },
      "@description <description>[]": {
        "@tag": {
          "type": "string",
          "default-value": "p"
        },
        "@modifiers": "array"
      },
      "<footer-left>": {},
      "<footer-right>": {},
      "@tag": {
        "type": "string",
        "default-value": "div"
      },
      "@header-modifiers": "array",
      "@footer-modifiers": "array",
      "@modifiers": "array",
      "@flush": {
        "type": "boolean",
        "default-value": false
      }
    },
    "endeavor-item-list": {
      "template": "./item/list",
      "<item>": {
        "@tag": {
          "type": "string",
          "default-value": "li"
        },
        "@modifiers": "array"
      },
      "<header>": {
        "@tag": {
          "type": "string",
          "default-value": "li"
        },
        "@modifiers": "array",
        "@href": "string",
        "@target": "string",
        "@title": "string"
      },
      "<description>": {
        "@tag": {
          "type": "string",
          "default-value": "li"
        },
        "@modifiers": "array",
        "@value": "string"
      },
      "@items": "array",
      "@tag": {
        "type": "string",
        "default-value": "ul"
      },
      "@flush": {
        "type": "string",
        "default-value": false
      },
      "@card": {
        "type": "string",
        "default-value": false
      },
      "@modifiers": "array"
    },
    "endeavor-item-list-wrapper": {
      "template": "./item/list-wrapper",
      "@card": {
        "type": "string",
        "default-value": false
      },
      "@flush": {
        "type": "string",
        "default-value": false
      },
      "@modifiers": "array",
      "@tag": {
        "type": "string",
        "default-value": "ul"
      }
    },
    "endeavor-item-list-header": {
      "template": "./item/list-header",
      "@modifiers": "array",
      "@tag": {
        "type": "string",
        "default-value": "li"
      },
      "@href": "string",
      "@target": "string",
      "@title": "string"
    },
    "endeavor-item-list-description": {
      "template": "./item/list-description",
      "@modifiers": "array",
      "@tag": {
        "type": "string",
        "default-value": "li"
      },
      "@value": "string"
    },
    "endeavor-item-list-node": {
      "template": "./item/list-node",
      "@modifiers": "array",
      "@node": "object",
      "@tag": {
        "type": "string",
        "default-value": "li"
      }
    }
  }
}
